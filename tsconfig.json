{
  "compilerOptions": {
      "strict": true,
      "declaration": true,
      "module": "ES2022",
      "target": "ES2022",
      "isolatedModules": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "sourceMap": true,
      "strictNullChecks": true,
      "baseUrl": ".",
      "moduleResolution": "Node",
      "paths": {
        "*": [
            "*",
            "types/*"
        ]
    }
  }
}
